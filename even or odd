#include <stdio.h>
#include <conio.h>

int main()
{
    int n;
    printf("enter any number:");
    scanf("%d", &n);
    if(n % 2 == 0 )
    {
        printf("The given number is an even number");
    }
    else
    {
       printf("The given number is  an odd number");
    }
    
    return 0;
    getch();
}
